
#include <stdio.h>

int main()
{
    int count=0;
    char a[20];
    printf("enter the string:");
    fgets(a,20,stdin);
    for(int i=0;a[i]!='\0';i++){
        if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u'){
            count++;
        }
    }
    printf("number of vowels=%d",count);
}
    
  
