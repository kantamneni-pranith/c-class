#include<stdio.h>
int main(){
    int a,b,g,s;
    scanf("%d%d",&a,&b);
    if(a>b){
        g=a;
        s=b;
    }
    else{
        g=b;
        s=a;
    }
    for(int i=g;i<=a*b;i+=g){
        if (i%s==0){
        printf("%d is the LCM",i);
        break;
            
        }
    }
    return 0; 
} 
