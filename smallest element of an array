#include<stdio.h>
int main(){
    int n,small,i;
    printf("enter number of elements:");
    scanf("%d",&n);
    int a[n];
    
    for(i=0;i<n;i++){
        printf("enter the element");
        scanf("%d",&a[i]);
    }
    small=a[0];
    for(i=0;i<n;i++){
        if(a[i]<small){
        small=a[i];
        }
    }
    printf("smallest element =%d",small);
    return 0;
}
