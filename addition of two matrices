
#include <stdio.h>

int main() {
    int m,n;
  int a[m][n],b[m][n],c[m][n];
  int i,j;
  printf("enter number of rows:");
  scanf("%d",&m);
  printf("enter number of columns:");
  scanf("%d",&n);
  printf("enter the valuesfor the matrix:\n");
for(i=0;i<m;i++){
    for(j=0;j<n;j++){
        printf("enter the elment a[%d][%d]:",i,j);
        scanf("%d",&a[i][j]);
    }
    printf("\n");
    
}   
for(i=0;i<m;i++){
    for(j=0;j<n;j++){
        printf("enter the elment b[%d][%d]",i,j);
        scanf("%d",&b[i][j]);
        c[i][j]=a[i][j]+b[i][j];
    }
    printf("\n");
    
} 
for(i=0;i<m;i++){
    for(j=0;j<n;j++){
        printf("\t%d",c[i][j]);
    
}printf("\n");}}
