  #include<stdio.h>
int main(){
    char str[20];
    printf("enter the string:");
    scanf("%s",&str);
    int i=0,dig=0,alp=0,spl=0;
    for(i=0;str[i]!='\0';i++){
        if((str[i]>='A'&&str[i]<='Z')||(str[i]>='a'&&str[i]<='z'))
        alp++;
        else if(str[i]>='0'&&str[i]<='9')
        dig++;
        else
        spl++;
    } 
    printf("alphabets=%d",alp);
    printf("\ndigits=%d",dig);
    printf("\nspecial charecters=%d",spl);



}
